DetailContainer from './movies/movie_detail_container'\nimport MoviesByGenreContainer from './movies/movies_by_genre_container'\nimport SearchContainer from './movies/search_container'\n\nconst App = () => {\n  return (\n    <div>\n      <header>\n        <GreetingContainer />\n        \n      </header>\n      <Switch>\n        <Route path='/signup' component={SignupFormContainer} />\n        <Route path='/login' component={LoginFormContainer} />\n        <Route exact path='/genre/:genreId' component={MoviesByGenreContainer} />\n      <Switch>\n        <Route path='/movie/:movie_id' component={MovieDetailContainer} />\n        <Route path='/' component={MovieIndexContainer} />\n      </Switch>\n    </Switch>\n    </div>\n  )\n}\n\nexport default App","/home/isaiah30/movie_app/client/src/frontend/components/greeting/greeting.jsx",["199","200"],"import React, { useState } from 'react'\nimport {Link} from 'react-router-dom'\nimport SearchContainer from '../movies/search_container'\nimport GenreListContainer from '../genre/genre_list_container'\nimport CouchIcon from '../../../images/couch_icon_ma.png'\nimport UserIcon from '../../../images/user_icon_ma.png'\nimport HamburgerIcon from '../../../images/hamburger.png'\n\nconst Greeting = (props) => {\n  const [isClicked, toggleHam] = useState(false)\n\n  const loggedIn = () => (\n    <div>\n      <h1>Welcome {props.currentUser.username}</h1>\n      <button onClick={() => props.logout()}>Logout</button>  \n    </div>\n  )\n\n  const loggedOut = () => (\n    <div>\n      <Link to='/signup'>\n        <button>Sign up</button>\n      </Link>\n      <Link to='/login'>\n        <button>Login</button>\n      </Link>\n    </div>\n  )\n  \n  return (\n    <>\n      <Link className='logo nav-section' to='/'>\n        <img className='icon' src={CouchIcon} alt=''/>\n        <h1>Couchflix</h1>\n      </Link>      \n      <SearchContainer />\n      <div className='nav-section'>\n        <div className='user-options'>\n          <img className='icon' src={UserIcon}/>\n          { props.currentUser.username ? loggedIn() : loggedOut() }\n          \n        </div>\n        <img \n          className={isClicked ? 'icon rotated' : 'icon'}\n          onClick={() => toggleHam(!isClicked)} \n          id='hamburger-icon' \n          src={HamburgerIcon}   \n        />\n        {isClicked ? <GenreListContainer /> : null}\n      </div>\n\n      \n    </>\n  )\n} \n\nexport default Greeting;","/home/isaiah30/movie_app/client/src/frontend/components/greeting/greeting_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/actions/session_actions.js",[],"/home/isaiah30/movie_app/client/src/frontend/util/session_api_util.js",[],"/home/isaiah30/movie_app/client/src/frontend/util/jquery-loader.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/session/login_form_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/session/session_form.jsx",[],"/home/isaiah30/movie_app/client/src/frontend/components/session/signup_form_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/movie_index.jsx",["201"],"import React, {useEffect} from 'react'\nimport {withRouter} from 'react-router'\nimport HorizontalMoviesByGenreContainer from './horizontal_movies_by_genre_container'\nimport FeaturedMoviesContainer from './featured_movies_container'\nimport '../../../stylesheets/carousels.scss'\n\nfunction MovieIndex(props) {\n  useEffect(() => {\n    props.fetchGenreList()\n  }, [])\n\n  return (\n    <>\n      <FeaturedMoviesContainer />\n      <div>\n        {Object.values(props.genres).map((genre) =>\n          <section className='carousels'>\n            <HorizontalMoviesByGenreContainer genre={genre}/>\n          </section>\n        )}   \n      </div>\n    </>\n  )\n}\n\nexport default withRouter(MovieIndex)","/home/isaiah30/movie_app/client/src/frontend/components/movies/movie_index_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/util/movie_api_util.js",[],"/home/isaiah30/movie_app/client/src/frontend/reducers/movie_reducer.js",[],"/home/isaiah30/movie_app/client/src/frontend/actions/movie_actions.js",[],"/home/isaiah30/movie_app/client/src/frontend/reducers/genre_reducer.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/movie_icon.jsx",["202","203"],"import React from 'react'\nimport { withRouter } from 'react-router'\nimport '../../../stylesheets/small_icons.scss'\nimport MovieDetailContainer from './movie_detail_container'\n// Should I render the detail view here or somewhere else?\n\nconst MovieIcon = ({clickable, history, movie: {poster_path, id}}) => {\n  const viewDetails = () => {\n    history.push(`/movie/${id}`)\n  }\n  try {\n    return (\n      <>\n        <div onClick={() => {\n          if (clickable) viewDetails()\n          }\n        } className='small_icons' id={id}>\n          <img src={`https://image.tmdb.org/t/p/w500${poster_path}`}/>\n        </div>\n      </>\n    )\n  } catch (error) {\n    return null\n  }\n\n}\n\nexport default withRouter(MovieIcon);","/home/isaiah30/movie_app/client/src/frontend/components/movies/movie_detail_container.js",[],["204","205"],"/home/isaiah30/movie_app/client/src/frontend/components/movies/movie_detail.jsx",["206","207","208"],"/home/isaiah30/movie_app/client/src/frontend/components/movies/featured_movies_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/featured_movies.jsx",["209"],"/home/isaiah30/movie_app/client/src/frontend/util/user_profile_util.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/layouts/homepage.jsx",[],["210","211"],"/home/isaiah30/movie_app/client/src/frontend/components/movies/continue_watching_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/horizontal_list.jsx",["212","213","214"],"import React, {useEffect} from 'react'\nimport MovieIcon from './movie_icon'\nimport Carousel from 'react-multi-carousel'\nimport 'react-multi-carousel/lib/styles.css';\nimport '../../../stylesheets/carousels.scss'\n\nfunction HorizontalList(props) {\n  useEffect(() => {\n    if (props.type === 'genre') {\n      props.fetchMoviesByGenre(props.genre.id)\n    }\n  }, [])\n\n  const responsive = {\n    superLargeDesktop: {\n      breakpoint: { max: 4000, min: 3000 },\n      items: 8,\n      slidesToSlide: 8\n    },\n    desktop: {\n      breakpoint: { max: 3000, min: 1024 },\n      items: 6,\n      slidesToSlide: 5\n    },\n    tablet: {\n      breakpoint: { max: 1024, min: 464 },\n      items: 3,\n      slidesToSlide: 3\n    },\n    mobile: {\n      breakpoint: { max: 464, min: 0 },\n      items: 1\n    }\n  };\n\n  let movies = []\n  switch (props.type) {\n    case ('Continue Watching'):\n      movies = props.continueWatching\n    case ('genre'):\n      if (props.genre) {\n        movies = props.movies[props.genre.id]\n      }\n  }\n  \n  try {\n    return ( \n    <>\n      {props.header ? <h2 className='carousels'>{props.header}</h2> : null}\n      <Carousel \n        className='carousels'\n        infinite={true}\n        responsive={responsive}>        \n          {\n            movies.map((movie, index) => {\n            return <MovieIcon clickable={true} id={index} movie={movie} />\n          }\n        )}\n      </Carousel>\n    </>\n  )\n  } catch (error) {\n    return null\n  }\n  \n}\n\nexport default HorizontalList","/home/isaiah30/movie_app/client/src/frontend/components/movies/featured_movie.jsx",["215"],"/home/isaiah30/movie_app/client/src/frontend/components/movies/movies_by_genre_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/search.jsx",["216"],"/home/isaiah30/movie_app/client/src/frontend/components/movies/search_results.jsx",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/movies_by_genre_page.jsx",["217","218"],"import React, { useEffect, useState, useCallback } from 'react'\nimport { withRouter } from 'react-router-dom'\nimport MovieIcon from './movie_icon'\nimport '../../../stylesheets/by-genre.scss'\n\nconst MoviesByGenrePage = ({movies, fetchMoviesByGenre, genre, match: {params}}, props) => {\n  const [page, setPage] = useState(1)\n  \n  const [isSending, setIsSending] = useState(false)\n  const sendRequest = useCallback(async (newPage) => {\n    // don't send again while we are sending\n  if (isSending) return\n  // update state\n  setIsSending(true)\n  // send the actual request\n  await fetchMoviesByGenre(params.genreId, newPage)\n  \n  // once the request is sent, update state again\n  setIsSending(false)\n  }, [isSending]) // update the callback if the state changes\n  \n  useEffect(() => {\n    fetchMoviesByGenre(params.genreId, page)\n  }, [])\n  \n  return <div className='movie-list'>\n    <h2>props.header</h2>\n    {\n      movies[params.genreId].map(movie => <MovieIcon movie={movie} clickable={true}/>)\n    }\n    <button \n      disabled={isSending} \n      onClick={() => {\n        window.scrollTo({\n          top: 0,\n          behavior: 'smooth'\n        })\n        let nextPage = page + 1\n        setPage(nextPage)\n        sendRequest(nextPage)\n        }\n      }>\n      More Movies{page}\n    </button>\n  </div>\n}\n\nexport default withRouter(MoviesByGenrePage)","/home/isaiah30/movie_app/client/src/frontend/components/genre/genre_list.jsx",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/horizontal_movies_by_genre_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/movies/search_container.js",[],"/home/isaiah30/movie_app/client/src/frontend/components/genre/genre_list_container.js",[],{"ruleId":"219","severity":1,"message":"220","line":1,"column":8,"nodeType":"221","messageId":"222","endLine":1,"endColumn":12},{"ruleId":"223","replacedBy":"224"},{"ruleId":"225","replacedBy":"226"},{"ruleId":"223","replacedBy":"227"},{"ruleId":"225","replacedBy":"228"},{"ruleId":"219","severity":1,"message":"229","line":9,"column":8,"nodeType":"221","messageId":"